                                                    Fill="{StaticResource DialogWindowDarkTransparentBrush}"
                                                    Opacity="1"
                                                    Stretch="Fill"/>
                                                <Path
                                                      Width="14"
                                                      Height="11"
                                                      Data="M 2,4.96 C2,4.96 7,4.96 7,4.96 7,4.96 7,9 7,9 7,9 2,9 2,9 2,9 2,4.96 2,4.96 zM 7,3 C7,3 7,1.96 7,1.96 7,1.96 12,1.96 12,1.96 12,1.96 12,6 12,6 12,6 9,6 9,6 9,6 9,3 9,3 9,3 7,3 7,3 zM 5,0 C5,0 5,3 5,3 5,3 0,3 0,3 0,3 0,11 0,11 0,11 9,11 9,11 9,11 9,7.93 9,7.93 9,7.93 14,7.93 14,7.93 14,7.93 14,0 14,0 14,0 5,0 5,0 z"
                                                      Fill="{StaticResource GlyphBackgroundBrush}"
                                                      Opacity="1"
                                                      Stretch="Fill"/>
                                            </Grid>
                                        </Grid>
                                    </Button>
                                    <Button
                                          x:Name="Maximize"
                                          IsEnabled="{TemplateBinding IsEnabled}"
                                          IsTabStop="False"
                                          Padding="0"
                                          Style="{StaticResource DialogButtonStyle}">
                                        <igBase:Commanding.Command>
                                            <igBase:XamDialogWindowCommandSource CommandType="ToggleMaximize" EventName="Click"/>
                                        </igBase:Commanding.Command>
                                        <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                                            <Grid x:Name="MaximizeGlyph">
                                                <Rectangle
                                                      Width="12"
                                                      Height="10"
                                                      Stroke="{StaticResource DialogWindowDarkTransparentBrush}"
                                                      StrokeThickness="4"/>
                                                <Rectangle
                                                      Width="10"
                                                      Height="8"
                                                      Stroke="{StaticResource GlyphBackgroundBrush}"
                                                      StrokeThickness="2"/>
                                            </Grid>
                                            <Grid x:Name="RestoreGlyph" Visibility="Collapsed">
                                                <Path
                                                    Width="16"
                                                    Height="13"
                                                    Data="M 4,7 C4,7 7,7 7,7 7,7 7,9 7,9 7,9 4,9 4,9 4,9 4,7 4,7 zM 11,4 C11,4 12,4 12,4 12,4 12,6 12,6 12,6 11,6 11,6 11,6 11,4 11,4 zM 0,3 C0,3 0,13 0,13 0,13 11,13 11,13 11,13 11,10 11,10 11,10 16,10 16,10 16,10 16,0 16,0 16,0 5,0 5,0 5,0 5,3 5,3 5,3 0,3 0,3 z"
                                                    Fill="{StaticResource DialogWindowDarkTransparentBrush}"
                                                    Opacity="1"
                                                    Stretch="Fill"/>
                                                <Path
                                                    Width="14"
                                                    Height="11"
                                                    Data="M 2,4.96 C2,4.96 7,4.96 7,4.96 7,4.96 7,9 7,9 7,9 2,9 2,9 2,9 2,4.96 2,4.96 zM 7,3 C7,3 7,1.96 7,1.96 7,1.96 12,1.96 12,1.96 12,1.96 12,6 12,6 12,6 9,6 9,6 9,6 9,3 9,3 9,3 7,3 7,3 zM 5,0 C5,0 5,3 5,3 5,3 0,3 0,3 0,3 0,11 0,11 0,11 9,11 9,11 9,11 9,7.93 9,7.93 9,7.93 14,7.93 14,7.93 14,7.93 14,0 14,0 14,0 5,0 5,0 z"
                                                    Fill="{StaticResource GlyphBackgroundBrush}"
                                                    Opacity="1"
                                                    Stretch="Fill"/>
                                            </Grid>
                                        </Grid>
                                    </Button>
                                    <Button
                                        x:Name="Close"
                                        Margin="1,0,0,0"
                                        IsEnabled="{TemplateBinding IsEnabled}"
                                        IsTabStop="False"
                                        Style="{StaticResource DialogButtonStyle}">
                                        <igBase:Commanding.Command>
                                            <igBase:XamDialogWindowCommandSource CommandType="Close" EventName="Click"/>
                                        </igBase:Commanding.Command>
                                        <Path
                                            Width="12"
                                            Height="10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Data="M0.5,0.5 L4.517,0.5 L6.062,3.125 L7.493,0.5 L11.5,0.5 L11.5,1.547 L8.742,6.120 L11.5,10.359 L11.5,11.5 L7.494,11.5 L6.062,8.874 L4.517,11.5 L0.5,11.5 L0.5,10.433 L3.305,6.120 L0.5,1.467 L0.5,0.5 z"
                                            Fill="{StaticResource DialogWindowLightTransparentBrush}"
                                            Stretch="Fill"
                                            Stroke="{StaticResource DialogWindowDarkTransparentBrush}"/>
                                    </Button>
                                </StackPanel>
                            </Border>
                        </igPrim:DialogRootPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
  <Style TargetType="igDW:XamDialogWindow" BasedOn="{StaticResource XamDialogWindowStyle}"/>
