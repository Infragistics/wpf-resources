              </VisualStateGroup>
              <VisualStateGroup x:Name="SpellCheckerStates">
                <VisualState x:Name="SpellCheckNormal">
                </VisualState>
                <VisualState x:Name="SpellCheckComplete">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SpellCheckCompleteGrid" Storyboard.TargetProperty="(UIElement.Visibility)">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Visible</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
