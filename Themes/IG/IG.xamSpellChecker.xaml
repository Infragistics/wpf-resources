              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <ig:SpellCheckerDialogRootPanel x:Name="Root">
              <!-- Borders -->
              <Grid x:Name="BorderGrid">
                <Border x:Name="Border1" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
                <Grid x:Name="ActiveResizeBorderGrid" Margin="0" Background="{x:Null}">
                  <Border x:Name="ActiveResizeBorder" Margin="1,1,1,1" Background="{StaticResource DialogWindowActiveResizeBackgroundBrush}" />
                </Grid>
                <Grid x:Name="NoActiveResizeBorderGrid" Margin="0" Background="{x:Null}" Visibility="Collapsed">
                  <Border x:Name="NoActiveResizeBorder" Margin="1,1,1,1" Background="{StaticResource DialogWindowNoActiveResizeBackgroundBrush}" CornerRadius="4,4,0,0" />
                </Grid>
              </Grid>
              <ContentControl x:Name="MaximizedContent" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" IsTabStop="False">
                <Grid>
                  <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" MinHeight="26" />
                    <RowDefinition />
                  </Grid.RowDefinitions>
                  <!-- Content -->
                  <Grid Margin="5,0,5,5" x:Name="ContentGrid" Grid.Row="1">
                    <Border BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" BorderBrush="{StaticResource DialogWindowContentBorderBrush}">
                      <Grid>
                        <Grid x:Name="Core" DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.CurrentBadWord}" Margin="{TemplateBinding Padding}">
                          <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                          </Grid.RowDefinitions>
                          <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="290" />
                            <ColumnDefinition Width="110" />
                          </Grid.ColumnDefinitions>
                          <StackPanel Orientation="Horizontal">
                            <TextBlock Margin="0,5,0,5" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.NotInDictionary}" TextWrapping="Wrap" />
                            <TextBlock Margin="5,5,0,5" Text="{Binding Word}" />
                          </StackPanel>
                          <StackPanel Grid.Row="1">
                            <TextBlock Margin="0,5,0,0" VerticalAlignment="Top" RenderTransformOrigin="1,1" Grid.Row="1" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Context}" TextWrapping="Wrap" Visibility="Collapsed"/>
                            <Border Margin="0,5,5,5" Background="{StaticResource ThemeLightForegroundBrush}" BorderBrush="{StaticResource DialogWindowContentInnerBorderBrush}" BorderThickness="1">
                              <ScrollViewer Height="80" BorderThickness="0" Style="{StaticResource ScrollViewerStyle}">
                                <TextBlock x:Name="Context" TextWrapping="Wrap" Margin="5" />
                              </ScrollViewer>
                            </Border>
                          </StackPanel>
                          <StackPanel Grid.Row="2">
                            <TextBlock Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Suggestions}" TextWrapping="Wrap" Margin="0,5,0,0" />
                            <ListBox x:Name="Suggestions" Height="92" Margin="0,5,5,5" SelectedItem="{Binding Mode=TwoWay, ElementName=ChangeTo, Path=Text}" Style="{StaticResource ListBoxStyle}" />
                            <TextBlock Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.UseThisInstead}" Margin="0,5,0,0" />
                            <TextBox x:Name="ChangeTo" TextWrapping="Wrap" Margin="0,5,5,5" Style="{StaticResource TextBoxStyle}" BorderBrush="{StaticResource DialogWindowContentInnerBorderBrush}" />
                          </StackPanel>
                          <StackPanel Margin="0,10,0,10" Grid.Row="3" Orientation="Horizontal">
                            <Button x:Name="PreviousField" HorizontalAlignment="Stretch" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.PreviousField}" Width="100" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="PreviousField" />
                              </igBase:Commanding.Command>
                            </Button>
                            <Button x:Name="NextField" HorizontalAlignment="Stretch" Margin="5,0,0,0" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.NextField}" Width="100" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="NextField" />
                              </igBase:Commanding.Command>
                            </Button>
                          </StackPanel>
                          <StackPanel Margin="5" Grid.Column="1" Grid.Row="1">
                            <Button x:Name="Ignore" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Ignore}" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="Ignore" />
                              </igBase:Commanding.Command>
                            </Button>
                            <Button x:Name="IgnoreAll" Margin="0,5,0,0" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.IgnoreAll}" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="IgnoreAll" />
                              </igBase:Commanding.Command>
                            </Button>
                          </StackPanel>
                          <StackPanel Margin="5,26,5,5" Grid.Column="1" Grid.Row="2">
                            <Button x:Name="Change" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Change}" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="Change" />
                              </igBase:Commanding.Command>
                            </Button>
                            <Button x:Name="ChangeAll" Margin="0,5,0,0" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.ChangeAll}" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="ChangeAll" />
                              </igBase:Commanding.Command>
                            </Button>
                          </StackPanel>
                          <Button Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Ok}" Grid.Row="3" Grid.Column="1" Margin="5,10" Style="{StaticResource ButtonStyle}">
                            <igBase:Commanding.Command>
                              <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="CancelDialog" />
                            </igBase:Commanding.Command>
                          </Button>
                        </Grid>
                        <Grid x:Name="SpellCheckCompleteGrid" Visibility="Collapsed" Margin="{TemplateBinding Padding}" HorizontalAlignment="Center" VerticalAlignment="Center">
                          <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                          </Grid.RowDefinitions>
                          <TextBlock Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.SpellCheckComplete}" HorizontalAlignment="Center" Margin="10,5">
                          </TextBlock>
                          <Button Width="100" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Ok}" Grid.Row="1" Margin="10,5" Style="{StaticResource ButtonStyle}">
                            <igBase:Commanding.Command>
                              <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="CloseDialog" />
                            </igBase:Commanding.Command>
                          </Button>
                        </Grid>
                      </Grid>
                    </Border>
                  </Grid>
                  <!-- Header -->
                  <Border x:Name="HeaderArea" Grid.Column="1" Background="{TemplateBinding HeaderBackground}" CornerRadius="5,5,0,0" Margin="1,1,1,0">
                    <Grid x:Name="WindowHeader" Margin="1,1,105,0" VerticalAlignment="Center">
                      <Grid HorizontalAlignment="Stretch">
                        <Grid.ColumnDefinitions>
                          <ColumnDefinition Width="Auto" />
                          <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <ContentControl Grid.Column="0" x:Name="SmallImage" ContentTemplate="{TemplateBinding HeaderIconTemplate}" MinHeight="16" MinWidth="16" MaxHeight="16" MaxWidth="16" Visibility="Visible" Margin="5,0,0,0" IsTabStop="False" VerticalAlignment="Center" />
                        <ContentControl x:Name="HeaderContent" Grid.Column="1" Margin="5,0,0,0" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" Foreground="{TemplateBinding HeaderForeground}" HorizontalContentAlignment="{TemplateBinding HeaderHorizontalContentAlignment}" VerticalContentAlignment="{TemplateBinding HeaderVerticalContentAlignment}" HorizontalAlignment="{TemplateBinding HeaderHorizontalContentAlignment}" VerticalAlignment="{TemplateBinding HeaderVerticalContentAlignment}" IsTabStop="False" />
                        <ContentControl x:Name="MinimizedHeaderContent" Visibility="Collapsed" Grid.Column="1" Margin="5,0,0,0" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding MinimizedHeaderTemplate}" Foreground="{TemplateBinding HeaderForeground}" HorizontalContentAlignment="{TemplateBinding HeaderHorizontalContentAlignment}" VerticalContentAlignment="{TemplateBinding HeaderVerticalContentAlignment}" HorizontalAlignment="{TemplateBinding HeaderHorizontalContentAlignment}" VerticalAlignment="{TemplateBinding HeaderVerticalContentAlignment}" IsTabStop="False" />
                      </Grid>
                    </Grid>
                  </Border>
                  <ContentControl x:Name="MinimizedContent" Visibility="Collapsed" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch" Width="{TemplateBinding MinimizedWidth}" Height="{TemplateBinding MinimizedHeight}" IsTabStop="False">
                    <Grid x:Name="MinimizedContentPanel" Margin="1,1,1,0" HorizontalAlignment="Stretch" VerticalAlignment="Top">
                      <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                      </Grid.ColumnDefinitions>
                      <ContentControl x:Name="MinimizedSmallImage" ContentTemplate="{TemplateBinding HeaderIconTemplate}" Grid.Column="0" MinHeight="16" MinWidth="16" MaxHeight="16" MaxWidth="16" Visibility="Visible" IsTabStop="False" />
                      <ContentControl Margin="7,3,8,0" HorizontalAlignment="Stretch" Grid.Column="1" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding MinimizedHeaderTemplate}" Foreground="{TemplateBinding HeaderForeground}" IsHitTestVisible="False" HorizontalContentAlignment="{TemplateBinding HeaderHorizontalContentAlignment}" VerticalContentAlignment="{TemplateBinding HeaderVerticalContentAlignment}" IsTabStop="False" />
                    </Grid>
                  </ContentControl>
                </Grid>
              </ContentControl>
              <Border x:Name="Resize" BorderThickness="7" BorderBrush="{StaticResource TransparentBrush}" />
              <!-- Buttons -->
              <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,5,0">
                <Button x:Name="Minimize" IsEnabled="{TemplateBinding IsEnabled}" IsTabStop="False" Padding="0" Style="{StaticResource DialogButtonStyle}">
                  <igBase:Commanding.Command>
                    <igBase:XamDialogWindowCommandSource CommandType="ToggleMinimize" EventName="Click" />
                  </igBase:Commanding.Command>
                  <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                    <Rectangle x:Name="MinimizeGlyph" Width="12" Height="5" HorizontalAlignment="Center" Margin="0,5,0,0" VerticalAlignment="Bottom" Fill="{StaticResource ThemeLightForegroundBrush}" Stroke="{StaticResource GlyphBackgroundBrush}" StrokeThickness="1" />
                    <Grid x:Name="MinimizeRestoreGlyph" Visibility="Collapsed">
                      <Path Width="16" Height="13" Data="M 4,7 C4,7 7,7 7,7 7,7 7,9 7,9 7,9 4,9 4,9 4,9 4,7 4,7 zM 11,4 C11,4 12,4 12,4 12,4 12,6 12,6 12,6 11,6 11,6 11,6 11,4 11,4 zM 0,3 C0,3 0,13 0,13 0,13 11,13 11,13 11,13 11,10 11,10 11,10 16,10 16,10 16,10 16,0 16,0 16,0 5,0 5,0 5,0 5,3 5,3 5,3 0,3 0,3 z" Fill="{StaticResource GlyphBackgroundBrush}" Opacity="1" Stretch="Fill" />
                      <Path Width="14" Height="11" Data="M 2,4.96 C2,4.96 7,4.96 7,4.96 7,4.96 7,9 7,9 7,9 2,9 2,9 2,9 2,4.96 2,4.96 zM 7,3 C7,3 7,1.96 7,1.96 7,1.96 12,1.96 12,1.96 12,1.96 12,6 12,6 12,6 9,6 9,6 9,6 9,3 9,3 9,3 7,3 7,3 zM 5,0 C5,0 5,3 5,3 5,3 0,3 0,3 0,3 0,11 0,11 0,11 9,11 9,11 9,11 9,7.93 9,7.93 9,7.93 14,7.93 14,7.93 14,7.93 14,0 14,0 14,0 5,0 5,0 z" Fill="{StaticResource ThemeLightForegroundBrush}" Opacity="1" Stretch="Fill" />
                    </Grid>
                  </Grid>
                </Button>
                <Button x:Name="Maximize" IsEnabled="{TemplateBinding IsEnabled}" IsTabStop="False" Padding="0" Style="{StaticResource DialogButtonStyle}">
                  <igBase:Commanding.Command>
                    <igBase:XamDialogWindowCommandSource CommandType="ToggleMaximize" EventName="Click" />
                  </igBase:Commanding.Command>
                  <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                    <Grid x:Name="MaximizeGlyph">
                      <Rectangle Width="12" Height="10" Stroke="{StaticResource GlyphBackgroundBrush}" StrokeThickness="4" />
                      <Rectangle Width="10" Height="8" Stroke="{StaticResource ThemeLightForegroundBrush}" StrokeThickness="2" />
                    </Grid>
                    <Grid x:Name="RestoreGlyph" Visibility="Collapsed">
                      <Path Width="16" Height="13" Data="M 4,7 C4,7 7,7 7,7 7,7 7,9 7,9 7,9 4,9 4,9 4,9 4,7 4,7 zM 11,4 C11,4 12,4 12,4 12,4 12,6 12,6 12,6 11,6 11,6 11,6 11,4 11,4 zM 0,3 C0,3 0,13 0,13 0,13 11,13 11,13 11,13 11,10 11,10 11,10 16,10 16,10 16,10 16,0 16,0 16,0 5,0 5,0 5,0 5,3 5,3 5,3 0,3 0,3 z" Fill="{StaticResource GlyphBackgroundBrush}" Opacity="1" Stretch="Fill" />
                      <Path Width="14" Height="11" Data="M 2,4.96 C2,4.96 7,4.96 7,4.96 7,4.96 7,9 7,9 7,9 2,9 2,9 2,9 2,4.96 2,4.96 zM 7,3 C7,3 7,1.96 7,1.96 7,1.96 12,1.96 12,1.96 12,1.96 12,6 12,6 12,6 9,6 9,6 9,6 9,3 9,3 9,3 7,3 7,3 zM 5,0 C5,0 5,3 5,3 5,3 0,3 0,3 0,3 0,11 0,11 0,11 9,11 9,11 9,11 9,7.93 9,7.93 9,7.93 14,7.93 14,7.93 14,7.93 14,0 14,0 14,0 5,0 5,0 z" Fill="{StaticResource ThemeLightForegroundBrush}" Opacity="1" Stretch="Fill" />
                    </Grid>
                  </Grid>
                </Button>
                <Button x:Name="Close" Margin="1,0,0,0" IsEnabled="{TemplateBinding IsEnabled}" IsTabStop="False" Style="{StaticResource DialogButtonStyle}">
                  <igBase:Commanding.Command>
                    <igBase:XamDialogWindowCommandSource CommandType="Close" EventName="Click" />
                  </igBase:Commanding.Command>
                  <Path Width="12" Height="10" HorizontalAlignment="Center" VerticalAlignment="Center" Data="M0.5,0.5 L4.517,0.5 L6.062,3.125 L7.493,0.5 L11.5,0.5 L11.5,1.547 L8.742,6.120 L11.5,10.359 L11.5,11.5 L7.494,11.5 L6.062,8.874 L4.517,11.5 L0.5,11.5 L0.5,10.433 L3.305,6.120 L0.5,1.467 L0.5,0.5 z" Fill="{StaticResource DialogWindowCloseButtonBackgroundBrush}" Stretch="Fill" Stroke="{StaticResource GlyphBackgroundBrush}" />
                </Button>
              </StackPanel>
            </ig:SpellCheckerDialogRootPanel>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

  <!-- ********************************** XamSpellChecker Style **********************************-->
  <Style TargetType="ig:XamSpellChecker">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="ig:XamSpellChecker">
