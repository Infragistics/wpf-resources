            <igPrim:DialogRootPanel x:Name="Root" IsHitTestVisible="{TemplateBinding IsEnabled}" Visibility="{Binding Visibility, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
              <!-- Borders -->
              <Grid x:Name="BorderGrid">
                <Border x:Name="Border1" Background="{StaticResource DialogWindowBackgroundBrush}" BorderBrush="{StaticResource DialogWindowBorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"/>
                <Grid x:Name="ActiveResizeBorderGrid" Margin="0" Background="{x:Null}">
                  <Border x:Name="ActiveResizeBorder" Background="{StaticResource DialogWindowActiveResizeBackgroundBrush}" BorderBrush="{StaticResource DialogWindowActiveResizeBorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"/>
                </Grid>
                <Grid x:Name="NoActiveResizeBorderGrid" Margin="0" Background="{x:Null}" Visibility="Collapsed">
                  <Border x:Name="NoActiveResizeBorder" Background="{StaticResource DialogWindowNoActiveResizeBackgroundBrush}" BorderBrush="{StaticResource DialogWindowNoActiveResizeBorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"/>
