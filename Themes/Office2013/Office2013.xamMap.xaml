  <!-- *********************************  Shadow Style  ********************************* -->
  <Style x:Key="ShadowStyle" TargetType="Path">
    <Setter Property="Fill" Value="{StaticResource ShadowStyleBackgroundBrush}" />
    <Setter Property="Stroke" Value="{StaticResource ShadowStyleBorderBrush}" />
    <Setter Property="StrokeThickness" Value="1" />
  </Style>
