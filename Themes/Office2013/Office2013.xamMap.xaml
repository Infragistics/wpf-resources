  <!-- *********************************  ShadowText Style  ********************************* -->
  <Style x:Key="ShadowTextStyle" TargetType="TextBlock" BasedOn="{StaticResource TextStyle}">
    <Setter Property="Foreground" Value="{StaticResource TransparentBrush}" />
