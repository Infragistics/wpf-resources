    <Setter Property="Background" Value="{StaticResource MapColorSwatchPaneBackgroundBrush}" />
    <Setter Property="BorderBrush" Value="{StaticResource MapColorSwatchPaneBorderBrush}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="Padding" Value="8" />
    <Setter Property="Margin" Value="5" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="local:MapColorSwatchPane">
          <Grid>
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="CommonStates">
                <VisualState x:Name="Normal">
                  <Storyboard>
                    <DoubleAnimation Duration="0:0:1.0" Storyboard.TargetName="Background" Storyboard.TargetProperty="Opacity" To="0.85" />
                    <DoubleAnimation Duration="0:0:2.0" Storyboard.TargetName="Foreground" Storyboard.TargetProperty="Opacity" To="0.85" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="MouseOver">
                  <Storyboard>
                    <DoubleAnimation Duration="0:0:0.4" Storyboard.TargetName="Background" Storyboard.TargetProperty="Opacity" To="1" />
                    <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="Foreground" Storyboard.TargetProperty="Opacity" To="1" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Rectangle StrokeThickness="1" Grid.Column="2" Fill="{StaticResource MapShadowRightBackgroundBrush}" />
            <Rectangle StrokeThickness="1" Fill="{StaticResource MapShadowLeftBackgroundBrush}" />
            <Border x:Name="Background" CornerRadius="0" Background="{TemplateBinding Background}" />
            <Border x:Name="Foreground" CornerRadius="0" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}" Grid.Column="1">
              <Grid>
                <Grid.RowDefinitions>
                  <RowDefinition Height="auto" />
                  <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <ContentPresenter Grid.Row="0" Content="{TemplateBinding Content}" />
                <ContentPresenter Grid.Row="1" x:Name="RootElement" />
              </Grid>
            </Border>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="local:MapColorSwatchPane" BasedOn="{StaticResource MapColorSwatchPaneStyle}" />

  <!-- *********************************  ThumbnailWorld Style  ********************************* -->
  <Style x:Key="ThumbnailWorldStyle" TargetType="Path">
    <Setter Property="Fill" Value="{StaticResource ThumbnailWorldBackgroundBrush}" />
    <Setter Property="Stroke" Value="{StaticResource ThumbnailWorldBorderBrush}" />
    <Setter Property="StrokeThickness" Value="1" />
  </Style>

  <!-- *********************************  ThumbnailWindow Style  ********************************* -->
  <Style x:Key="ThumbnailWindowStyle" TargetType="Path">
    <Setter Property="Fill" Value="{StaticResource ThumbnailWindowBackgroundBrush}" />
    <Setter Property="Stroke" Value="{StaticResource ThumbnailWindowBorderBrush}" />
    <Setter Property="StrokeThickness" Value="1" />
  </Style>

  <!-- *********************************  MapThumbnailPane Style  ********************************* -->
  <Style x:Key="MapThumbnailPaneStyle" TargetType="local:MapThumbnailPane">
    <Setter Property="WorldStyle" Value="{StaticResource ThumbnailWorldStyle}" />
    <Setter Property="WindowStyle" Value="{StaticResource ThumbnailWindowStyle}" />
    <Setter Property="PreviewStyle" Value="{StaticResource PreviewStyle}" />
    <Setter Property="Background" Value="{StaticResource MapThumbnailPaneBackgroundBrush}" />
    <Setter Property="BorderBrush" Value="{StaticResource MapThumbnailPaneBorderBrush}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="Padding" Value="3" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="local:MapThumbnailPane">
          <Grid x:Name="RootVisual" SnapsToDevicePixels="True">
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="CommonStates">
                <VisualState x:Name="Normal">
                  <Storyboard>
                    <DoubleAnimation Duration="0:0:1.0" Storyboard.TargetName="BackgroundVisual" Storyboard.TargetProperty="Opacity" To="0.6" />
                    <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="MouseOverVisual" Storyboard.TargetProperty="Opacity" To="0.0" />
                    <DoubleAnimation Duration="0:0:2.0" Storyboard.TargetName="RootElementVisual" Storyboard.TargetProperty="Opacity" To="0.8" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="MouseOver">
                  <Storyboard>
                    <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="BackgroundVisual" Storyboard.TargetProperty="Opacity" To="1" />
                    <DoubleAnimation Duration="0:0:1.0" Storyboard.TargetName="MouseOverVisual" Storyboard.TargetProperty="Opacity" To="1.0" />
                    <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="RootElementVisual" Storyboard.TargetProperty="Opacity" To="1" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
              <VisualStateGroup x:Name="ActionStates">
                <VisualState x:Name="Idle">
                  <Storyboard />
                </VisualState>
                <VisualState x:Name="Panning">
                  <Storyboard>
                    <DoubleAnimation Duration="0:0:0.1" Storyboard.TargetName="PressedVisual" Storyboard.TargetProperty="Opacity" To="1.0" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
              <VisualStateGroup x:Name="FocusStates">
                <VisualState x:Name="Focused">
                  <Storyboard>
                    <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="FocusVisual" Storyboard.TargetProperty="Opacity" To="1.0" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Unfocused">
                  <Storyboard>
                    <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="FocusVisual" Storyboard.TargetProperty="Opacity" To="0.0" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Rectangle StrokeThickness="1" Grid.Column="2" Fill="{StaticResource MapShadowRightBackgroundBrush}" />
            <Rectangle StrokeThickness="1" Fill="{StaticResource MapShadowLeftBackgroundBrush}" />
            <Grid x:Name="BackgroundVisual" Grid.Column="1">
              <Border x:Name="NormalVisual" CornerRadius="0" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
              <Border x:Name="MouseOverVisual" CornerRadius="0" Opacity="0" Background="{StaticResource MapThumbnailPaneHoverBackgroundBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
              <Border x:Name="PressedVisual" CornerRadius="0" Opacity="0" Background="{StaticResource MapThumbnailPaneHoverBackgroundBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
              <Border x:Name="FocusVisual" CornerRadius="0" Opacity="0" BorderThickness="{TemplateBinding BorderThickness}" />
            </Grid>
            <Border x:Name="RootElementVisual" Opacity="1" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}" Grid.Column="1">
              <Grid>
                <Grid.RowDefinitions>
                  <RowDefinition Height="auto" />
                  <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <ContentPresenter Grid.Row="0" Content="{TemplateBinding Content}" />
                <ContentPresenter Grid.Row="1" x:Name="RootElement" />
              </Grid>
            </Border>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="local:MapThumbnailPane" BasedOn="{StaticResource MapThumbnailPaneStyle}" />

  <!-- *********************************  ViewportDrag Style  ********************************* -->
  <Style x:Key="ViewportDragStyle" TargetType="Path">
    <Setter Property="StrokeThickness" Value="1" />
    <Setter Property="Stroke" Value="{StaticResource ViewportDragBorderBrush}" />
  </Style>

  <!-- *********************************  MapViewport Style  ********************************* -->
  <Style x:Key="MapViewportStyle" TargetType="local:MapViewport">
    <Setter Property="DragStyle" Value="{StaticResource ViewportDragStyle}" />
    <Setter Property="PreviewStyle" Value="{StaticResource PreviewStyle}" />
    <Setter Property="GridStroke" Value="{StaticResource GridStrokeBorderBrush}" />
    <Setter Property="GridStrokeThickness" Value="1" />
    <Setter Property="GridShadowStroke" Value="{StaticResource MapViewportGridShadowBorderBrush}" />
    <Setter Property="GridShadowOffset" Value="-0.75 -0.75" />
    <Setter Property="Background" Value="{StaticResource MapViewportBackgroundBrush}" />
    <Setter Property="BorderBrush" Value="{StaticResource MapViewportBorderBrush}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="Padding" Value="0" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="local:MapViewport">
          <Grid>
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="CommonStates">
                <VisualState x:Name="Normal" />
                <VisualState x:Name="MouseOver" />
              </VisualStateGroup>
              <VisualStateGroup x:Name="ActionStates">
                <VisualState x:Name="Idle" />
                <VisualState x:Name="Panning" />
                <VisualState x:Name="Zooming" />
                <VisualState x:Name="Selecting" />
              </VisualStateGroup>
              <VisualStateGroup x:Name="FocusStates">
                <VisualState x:Name="Focused">
                  <Storyboard>
                    <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="FocusVisual" Storyboard.TargetProperty="Opacity" To="1.0" />
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="Unfocused">
                  <Storyboard>
                    <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="FocusVisual" Storyboard.TargetProperty="Opacity" To="0.0" />
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" />
            <Grid Margin="{TemplateBinding Padding}">
              <ContentPresenter x:Name="RootElement" />
            </Grid>
            <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
            <Border x:Name="FocusVisual" Opacity="0" BorderBrush="{StaticResource MapViewportFocusedBorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="local:MapViewport" BasedOn="{StaticResource MapViewportStyle}" />

  <!-- *********************************  BubbleGauge Style  ********************************* -->
  <Style x:Key="BubbleGaugeStyle" TargetType="local:BubbleGauge">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="local:BubbleGauge">
          <Border Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
            <ContentPresenter HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Content="{TemplateBinding Content}">
            </ContentPresenter>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="local:BubbleGauge" BasedOn="{StaticResource BubbleGaugeStyle}" />

  <!-- *********************************  MapLayer Style  ********************************* -->
  <Style x:Key="MapLayerStyle" TargetType="local:MapLayer">
    <Setter Property="Foreground" Value="{StaticResource MapForegroundBrush}" />
    <Setter Property="FillMode" Value="RandomSelect" />
    <Setter Property="ShadowFill" Value="{StaticResource MapLayerShadowBackgroundBrush}" />
    <Setter Property="Stroke" Value="{StaticResource MapLayerBorderBrush}" />
    <Setter Property="StrokeThickness" Value="1" />
    <Setter Property="Brushes">
      <Setter.Value>
        <ig:BrushCollection>
          <SolidColorBrush Color="{StaticResource Color_013}" />
          <SolidColorBrush Color="{StaticResource Color_012}" />
          <SolidColorBrush Color="{StaticResource Color_011}" />
        </ig:BrushCollection>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="local:MapLayer" BasedOn="{StaticResource MapLayerStyle}" />

  <!-- *********************************  Map Style  ********************************* -->
  <Style x:Key="XamMapStyle" TargetType="local:XamMap">
    <Setter Property="Foreground" Value="{StaticResource MapForegroundBrush}" />
    <Setter Property="WindowZoomMaximum" Value="15" />
    <Setter Property="Background" Value="{StaticResource MapBackgroundBrush}" />
    <Setter Property="BorderBrush" Value="{StaticResource MapBorderBrush}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="ViewportBackground" Value="{StaticResource MapControlViewportBackgroundBrush}" />
    <Setter Property="ViewportBorderBrush" Value="{StaticResource MapControlViewportBorderBrush}" />
    <Setter Property="GridDisplayMode" Value="None" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="local:XamMap">
          <Border Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Padding="{TemplateBinding Padding}">
            <ContentPresenter x:Name="RootElement">
            </ContentPresenter>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="local:XamMap" BasedOn="{StaticResource XamMapStyle}" />
</ResourceDictionary>