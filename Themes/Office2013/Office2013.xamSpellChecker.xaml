                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="MaximizeButtonIsNotVisible">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Maximize" Storyboard.TargetProperty="Visibility">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Collapsed</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
              <VisualStateGroup x:Name="MinimizeButtonStates">
                <VisualState x:Name="MinimizeButtonIsVisible">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Minimize" Storyboard.TargetProperty="Visibility">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Visible</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="MinimizeButtonIsNotVisible">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Minimize" Storyboard.TargetProperty="Visibility">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Collapsed</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
              <VisualStateGroup x:Name="HeaderIconStates">
                <VisualState x:Name="HeaderIconIsNotVisible">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SmallImage" Storyboard.TargetProperty="Visibility">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Collapsed</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
                <VisualState x:Name="HeaderIconIsVisible">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SmallImage" Storyboard.TargetProperty="Visibility">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>Visible</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
              <VisualStateGroup x:Name="SpellCheckerStates">
                <VisualState x:Name="SpellCheckNormal">
                </VisualState>
                <VisualState x:Name="SpellCheckComplete">
                  <Storyboard>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SpellCheckCompleteGrid" Storyboard.TargetProperty="(UIElement.Visibility)">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>
                            Visible
                          </Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Core" Storyboard.TargetProperty="(UIElement.Visibility)">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>
                            Collapsed
                          </Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Footer" Storyboard.TargetProperty="(UIElement.Visibility)">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>
                            Collapsed
                          </Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Close" Storyboard.TargetProperty="(UIElement.Visibility)">
                      <DiscreteObjectKeyFrame KeyTime="0">
                        <DiscreteObjectKeyFrame.Value>
                          <Visibility>
                            Collapsed
                          </Visibility>
                        </DiscreteObjectKeyFrame.Value>
                      </DiscreteObjectKeyFrame>
                    </ObjectAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <igPrim:DialogRootPanel x:Name="Root" IsHitTestVisible="{TemplateBinding IsEnabled}" Visibility="{Binding Visibility, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
              <!-- Borders -->
              <Grid x:Name="BorderGrid">
                <Grid x:Name="ActiveResizeBorderGrid" Margin="0" Background="{x:Null}">
                  <Border x:Name="ActiveResizeBorder" BorderBrush="{StaticResource DialogWindowActiveResizeBorderBrush}" BorderThickness="1" />
                </Grid>
                <Grid x:Name="NoActiveResizeBorderGrid" Margin="0" Background="{x:Null}" Visibility="Collapsed">
                  <Border x:Name="NoActiveResizeBorder" BorderBrush="{StaticResource DialogWindowNoActiveResizeBorderBrush}" BorderThickness="1" />
                </Grid>
              </Grid>
              <ContentControl x:Name="MaximizedContent" HorizontalContentAlignment="Stretch" IsEnabled="{TemplateBinding IsEnabled}" IsTabStop="False" VerticalContentAlignment="Stretch" Visibility="Visible">
                <Grid Background="{TemplateBinding Background}" Margin="{TemplateBinding BorderThickness}">
                  <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" MinHeight="36" />
                    <RowDefinition />
                    <RowDefinition Height="Auto" MinHeight="42" />
                  </Grid.RowDefinitions>
                  <!-- Content -->
                  <Grid Margin="1,0,1,1" x:Name="ContentGrid" Grid.Row="1">
                    <Border BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" BorderBrush="{StaticResource DialogWindowContentBorderBrush}">
                      <Grid>
                        <Grid x:Name="Core" DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.CurrentBadWord}" Margin="16,0,16,0">
                          <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                          </Grid.RowDefinitions>
                          <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" MinWidth="70" />
                          </Grid.ColumnDefinitions>
                          <StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="0,0,0,6">
                              <TextBlock Style="{StaticResource TextBlockStyle}" Foreground="{StaticResource DialogWindowHeaderForegroundBrush}" Margin="0,0,0,0" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.NotInDictionary}" TextWrapping="Wrap" />
                              <TextBlock Style="{StaticResource TextBlockStyle}" Margin="5,0,0,0" Text="{Binding Word}" />
                            </StackPanel>
                            <TextBlock Margin="0,0,0,8" Style="{StaticResource TextBlockStyle}" VerticalAlignment="Top" RenderTransformOrigin="1,1" Grid.Row="1" TextWrapping="Wrap" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Context}" Visibility="Collapsed" />
                          </StackPanel>
                          <StackPanel Grid.Row="1" Margin="0,0,10,0">
                            <Border Margin="0" Background="{StaticResource LightForegroundBrush}" BorderBrush="{StaticResource DialogWindowContentInnerBorderBrush}" BorderThickness="1">
                              <ScrollViewer Height="88" Style="{StaticResource ScrollViewerStyle}">
                                <TextBlock x:Name="Context" TextWrapping="Wrap" Style="{StaticResource ContentTextBlockStyle}" Margin="6,4" />
                              </ScrollViewer>
                            </Border>
                          </StackPanel>
                          <StackPanel Grid.Row="2" Margin="0,8,10,0">
                            <TextBlock Style="{StaticResource TextBlockStyle}" Foreground="{StaticResource DialogWindowHeaderForegroundBrush}" Margin="0,0,0,6" TextWrapping="Wrap" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Suggestions}" />
                            <ListBox x:Name="Suggestions" Height="88" SelectedItem="{Binding Mode=TwoWay, ElementName=ChangeTo, Path=Text}" Style="{StaticResource ListBoxStyle}" />
                            <TextBlock Margin="0,10,0,6" Style="{StaticResource TextBlockStyle}" Foreground="{StaticResource DialogWindowHeaderForegroundBrush}" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.UseThisInstead}" />
                            <TextBox x:Name="ChangeTo" TextWrapping="Wrap" Style="{StaticResource TextBoxStyle}" />
                          </StackPanel>
                          <StackPanel Margin="0,10,10,0" Grid.Row="3" Orientation="Horizontal">
                            <Button x:Name="PreviousField" HorizontalAlignment="Stretch" MinWidth="90" Style="{StaticResource ButtonStyle}" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.PreviousField}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="PreviousField" />
                              </igBase:Commanding.Command>
                            </Button>
                            <Button x:Name="NextField" HorizontalAlignment="Stretch" Margin="8,0,0,0" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.NextField}" MinWidth="90" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="NextField" />
                              </igBase:Commanding.Command>
                            </Button>
                          </StackPanel>
                          <StackPanel Grid.Column="1" Grid.Row="1">
                            <Button x:Name="Ignore" Style="{StaticResource ButtonStyle}" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Ignore}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="Ignore" />
                              </igBase:Commanding.Command>
                            </Button>
                            <Button x:Name="IgnoreAll" Margin="0,8,0,0" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.IgnoreAll}" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="IgnoreAll" />
                              </igBase:Commanding.Command>
                            </Button>
                          </StackPanel>
                          <StackPanel Margin="0,32,0,0" Grid.Column="1" Grid.Row="2">
                            <Button x:Name="Change" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Change}" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="Change" />
                              </igBase:Commanding.Command>
                            </Button>
                            <Button x:Name="ChangeAll" Margin="0,8,0,0" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.ChangeAll}" Style="{StaticResource ButtonStyle}">
                              <igBase:Commanding.Command>
                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="ChangeAll" />
                              </igBase:Commanding.Command>
                            </Button>
                          </StackPanel>
                        </Grid>
                        <Grid x:Name="SpellCheckCompleteGrid" Visibility="Collapsed" Margin="{TemplateBinding Padding}" HorizontalAlignment="Center" VerticalAlignment="Center">
                          <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                          </Grid.RowDefinitions>
                          <TextBlock Style="{StaticResource TextBlockStyle}" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.SpellCheckComplete}" HorizontalAlignment="Center" Margin="10,5">
                          </TextBlock>
                          <Button Width="120" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Ok}" Grid.Row="1" Margin="10,5" Style="{StaticResource ButtonStyle}">
                            <igBase:Commanding.Command>
                              <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="CloseDialog" />
                            </igBase:Commanding.Command>
                          </Button>
                        </Grid>
                      </Grid>
                    </Border>
                  </Grid>
