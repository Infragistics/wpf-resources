                      </Grid>
                    </Grid>
                  </Border>
                  <Border x:Name="Footer" Height="42" Margin="1,0,1,1" Grid.Row="2" HorizontalAlignment="Stretch" Background="{StaticResource DialogWindowFooterBackgroundBrush}">
                    <Button Margin="0,0,17,0" Width="70" HorizontalAlignment="Right" VerticalAlignment="Center" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Ok}" Style="{StaticResource ButtonStyle}">
                      <igBase:Commanding.Command>
                        <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="CancelDialog" />
                      </igBase:Commanding.Command>
                    </Button>
                  </Border>
                </Grid>
              </ContentControl>
              <Border x:Name="Resize" BorderThickness="23" Margin="-18" BorderBrush="{StaticResource TransparentBrush}" />
              <!-- Buttons -->
              <Border HorizontalAlignment="Right" Margin="0,4,5,0" VerticalAlignment="Top">
                <StackPanel Orientation="Horizontal" Margin="0,6,6,0">
                  <Button x:Name="Minimize" IsEnabled="{TemplateBinding IsEnabled}" IsTabStop="False" Padding="0" Style="{StaticResource DialogButtonStyle}" Margin="0,0,7,0">
                    <igDW:Commanding.Command>
                      <igDW:XamDialogWindowCommandSource CommandType="ToggleMinimize" EventName="Click" />
                    </igDW:Commanding.Command>
                    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                      <Rectangle x:Name="MinimizeGlyph" Fill="{Binding Path=Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ContentControl}}" Height="2" VerticalAlignment="Bottom" Width="9" Margin="0 7 0 0" igPrim:XamlHelper.SnapsToDevicePixels="True" />
