                           MinHeight="16"
                           MinWidth="16"
                           MaxHeight="16"
                           MaxWidth="16"
                           Visibility="Visible"
                           Margin="5,0,0,0"
                           IsTabStop="False"
                           VerticalAlignment="Center"/>
