  <SolidColorBrush x:Key="DialogWindowHeaderForegroundBrush" Color="{StaticResource Color_023}"/>
  <SolidColorBrush x:Key="DialogWindowFocusVisualStyleBorderBrush" Color="{StaticResource Color_026}"/>
  <SolidColorBrush x:Key="DialogWindowFocusVisualStyleBackgroundBrush" Color="{StaticResource Color_033}"/>
  <SolidColorBrush x:Key="InnerHighlightBorderBrush" Color="{StaticResource Color_031}"/>
