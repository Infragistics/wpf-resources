    <!-- *********************************  OPDPaneButton Style  ********************************* -->
    <Style x:Key="OPDPaneButtonStyle" TargetType="Button">
        <Setter Property="Background" Value="{StaticResource OPDButtonBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource OPDButtonBorderBrush}"/>
        <Setter Property="Foreground" Value="{StaticResource ForegroundBrush}"/>
        <Setter Property="Padding" Value="2,0,2,0"/>
