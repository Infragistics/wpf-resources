                        <Ellipse x:Name="FocusedVisualElement" Stroke="{StaticResource OPDButtonFocusedBorderBrush}" StrokeThickness="1"  Opacity="0"/>
                        <Ellipse x:Name="DisabledVisualElement" Fill="{StaticResource DisabledBackgroundBrush}" IsHitTestVisible="false" Opacity="0"/>
