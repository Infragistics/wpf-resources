     <!-- *********************************  PercentageThumb Style  ********************************* -->
    <Style x:Key="PercentageThumbStyle" TargetType="Thumb">
        <Setter Property="Background" Value="{StaticResource SliderThumbBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource SliderThumbBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Grid Height="18" Width="11">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="0.6" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ThumbInnerStroke"/>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Fill" Storyboard.TargetName="ThumbBackground" >
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource SliderThumbHoverBackgroundBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Stroke" Storyboard.TargetName="ThumbBackground" >
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource SliderThumbHoverBorderBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Fill" Storyboard.TargetName="ThumbBackground" >
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource SliderThumbPressedBackgroundBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Stroke" Storyboard.TargetName="ThumbBackground" >
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource SliderThumbPressedBorderBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="0.55" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ThumbDisabled" d:IsOptimized="True"/>
                                        <DoubleAnimation Duration="0" To="0.55" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ThumbTip" d:IsOptimized="True"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused"/>
                                <VisualState x:Name="Unfocused"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid Margin="1,2,1,0" Width="9" Height="13">
                            <Canvas Width="9" Height="13">
                                <Path x:Name="ThumbBackground" Data="M0.5,0.5 L8.5,0.5 L8.5,9.9374847 L4.5208335,13.31249 L0.5,9.9374847 z" Fill="{TemplateBinding Background}" Stretch="Fill" Stroke="{TemplateBinding BorderBrush}" UseLayoutRounding="False" Height="13" Width="9"/>
                                <Path x:Name="ThumbInnerStroke" Data="M0.5,0.5 L8.5,0.5 L8.5,9.9374847 L4.5208335,12.972734 L0.5,9.9374847 z" Stretch="Fill" Stroke="{StaticResource SliderThumbHoverInnerBorderBrush}" UseLayoutRounding="False" Height="11" Width="7" Margin="1" Opacity="0"/>
                                <Path x:Name="ThumbFocused" Data="M0.5,0.5 L8.5,0.5 L8.5,9.9374847 L4.5208335,13.31249 L0.5,9.9374847 z" Stretch="Fill" Fill="{StaticResource SliderThumbPressedBackgroundBrush}" Stroke="{StaticResource SliderThumbPressedBorderBrush}" UseLayoutRounding="False" Height="13" Width="9" Visibility="Collapsed"/>
                                <Path x:Name="ThumbDisabled" Data="M0.5,0.5 L8.5,0.5 L8.5,9.9374847 L4.5208335,13.31249 L0.5,9.9374847 z" Stretch="Fill" UseLayoutRounding="False" Height="13" Width="9" Fill="{StaticResource SliderThumbDisabledBackgroundBrush}" Opacity="0"/>
                                <Grid x:Name="ThumbTip" Height="22" Canvas.Left="-14" Canvas.Top="-22" HorizontalAlignment="Center">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition  Width="6"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="6"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="17" />
                                        <RowDefinition Height="8" />
                                    </Grid.RowDefinitions>
                                    <Border Grid.Column="2" Background="{StaticResource ThumbTooltipBackgroundBrush}" Height="17" UseLayoutRounding="False" VerticalAlignment="Top" HorizontalAlignment="Right" Width="6" RenderTransformOrigin="0.5,0.5" CornerRadius="0,3,3,0" BorderThickness="0,1,1,1" BorderBrush="{StaticResource ThumbTooltipBorderBrush}"/>
                                    <Border Grid.Column="0" Background="{StaticResource ThumbTooltipBackgroundBrush}"  Height="17"  UseLayoutRounding="False" VerticalAlignment="Top" HorizontalAlignment="Left" Width="6" CornerRadius="3,0,0,3" BorderThickness="1,1,0,1" BorderBrush="{StaticResource ThumbTooltipBorderBrush}"/>
                                    <Border Grid.Column="1" Height="17" VerticalAlignment="Top" BorderThickness="0,1" Background="{StaticResource ThumbTooltipBackgroundBrush}" BorderBrush="{StaticResource ThumbTooltipBorderBrush}"/>
                                    <Path Grid.Column="1" Grid.Row="1" Data="M22.804562,18 L22.613688,18.159668 C20.847961,19.636719 18.0228,22 18.0228,22 C18.0228,22 15.176086,19.636719 13.39689,18.159668 L13.20456,18 z" Stretch="Fill" UseLayoutRounding="False" Height="5.5" Width="10" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="7,-1,0,0" Fill="{StaticResource TooltipPointBackgroundBrush}" />
                                    <StackPanel Grid.Column="1" Orientation="Horizontal" VerticalAlignment="Center">
                                        <TextBlock x:Name="textBlock" Text="{Binding ZoomLevelDisplayText}" TextWrapping="NoWrap" 
                                                   HorizontalAlignment="Center" VerticalAlignment="Center" 
                                                   Foreground="{StaticResource ThemeForegroundBrush}" FontSize="10" FontFamily="Arial"/>
                                        <TextBlock Text="%" TextWrapping="NoWrap" 
                                                   HorizontalAlignment="Center" VerticalAlignment="Center" 
                                                   Foreground="{StaticResource ThemeForegroundBrush}" FontSize="8" FontFamily="Arial"/>
                                    </StackPanel>
                                </Grid>
                            </Canvas>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- *********************************  PercentageSlider Style  ********************************* -->
    <Style x:Key="PercentageSliderStyle" TargetType="Slider">
        <Setter Property="Stylus.IsPressAndHoldEnabled" Value="false"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Slider">
                    <ControlTemplate.Resources>
                        <ControlTemplate x:Key="RepeatButtonTemplate">
                            <Grid x:Name="Root" Background="{StaticResource TransparentBrush}" Opacity="0"/>
                        </ControlTemplate>
                    </ControlTemplate.Resources>
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="true">
                        <Grid Margin="0,0,5,0">
                            <TickBar x:Name="TopTick" Fill="{TemplateBinding Foreground}" Height="4" Placement="Top" Grid.Row="0" Visibility="Collapsed"/>
                            <TickBar x:Name="BottomTick" Fill="{TemplateBinding Foreground}" Height="4" Placement="Bottom" Grid.Row="2" Visibility="Collapsed"/>
                            <Border x:Name="TrackBackground" Background="{StaticResource HSliderTrackBackgroundBrush}" Height="2" Grid.Row="1" VerticalAlignment="center">
                                <Canvas Margin="-6,0">
                                    <Rectangle x:Name="PART_SelectionRange" Fill="{StaticResource TransparentBrush}" Height="4.0" 
