    <!-- *********************************  XamZoombar Style  ********************************* -->
    <Style x:Key="XamZoombarStyle" TargetType="local:XamZoombar">
        <Setter Property="MinWidth" Value="20" />
        <Setter Property="MinHeight" Value="20" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:XamZoombar">
                    <Grid igPrim:XamlHelper.SnapsToDevicePixels="True">
                        <Canvas x:Name="HorizontalRootCanvasElement" Background="{TemplateBinding Background}">
                            <Grid x:Name="HorizontalScrollbarElement">
                                <Border Background="{StaticResource HScrollbarElementBackgroundBrush}" BorderBrush="{StaticResource HScrollbarElementBorderBrush}" BorderThickness="1" CornerRadius="2">
                                    <Border BorderBrush="{StaticResource HScrollbarElementInnerHighlightBorderBrush}" BorderThickness="1" CornerRadius="2"/>
                                </Border>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Border x:Name="HorizontalTrackElement" BorderThickness="1,0,1,1" Background="{StaticResource TransparentBrush}" Grid.Column="1" CornerRadius="2" Margin="0,2,0,1">
                                        <Border BorderThickness="1" CornerRadius="2" Margin="0,1,0,0">
                                            <Border BorderThickness="1" CornerRadius="0,0,1,1"/>
                                        </Border>
                                    </Border>
                                    <Grid Cursor="Hand">
                                        <ContentControl x:Name="HorizontalScrollLeftElement" Style="{StaticResource HorizontalScrollLeftStyle}">
                                            <Grid MaxWidth="9" MaxHeight="22" MinWidth="3" MinHeight="6">
                                                <Path Data="M 8,11 C8,11 19,0 19,0 19,0 19,22 19,22 19,22 8,11 8,11 z" Fill="{StaticResource OPDGlyphsBackgroundBrush}" HorizontalAlignment="Center" Opacity="1" Stretch="Fill" VerticalAlignment="Center" Width="6" Height="9"/>
                                            </Grid>
                                        </ContentControl>
                                    </Grid>
                                    <Grid Cursor="Hand" Grid.Column="2">
                                        <ContentControl x:Name="HorizontalScrollRightElement" Style="{StaticResource HorizontalScrollRightStyle}">
                                            <Grid MaxWidth="9" MaxHeight="22" MinWidth="3" MinHeight="6">
                                                <Path Data="M 11,11 C11,11 0,0 0,0 0,0 0,22 0,22 0,22 11,11 11,11 z" Fill="{StaticResource OPDGlyphsBackgroundBrush}" HorizontalAlignment="Center" Opacity="1" Stretch="Fill" VerticalAlignment="Center" Width="6" Height="9"/>
                                            </Grid>
                                        </ContentControl>
                                    </Grid>
                                </Grid>
                                <Grid x:Name="HorizontalPreviewElement" Margin="17,6,17,6">
                                    <ContentPresenter Content="{TemplateBinding HorizontalPreviewContent}"/>
                                </Grid>
                            </Grid>
                            <ContentControl x:Name="HorizontalThumbElement" Cursor="Hand" HorizontalContentAlignment="Stretch" Style="{TemplateBinding HorizontalThumbStyle}" UseLayoutRounding="False" VerticalContentAlignment="Stretch" Height="19">
                                <Grid>
                                    <Border BorderThickness="0,1,0,1" Margin="0,2" UseLayoutRounding="False" BorderBrush="{StaticResource HThumbOuterBorderBrush}">
                                        <Border BorderBrush="{StaticResource HThumbBorderBrush}" BorderThickness="0,2" Opacity="1">
                                            <Border BorderThickness="1" Background="{StaticResource HThumbBackgroundBrush}" Margin="4,0" BorderBrush="{StaticResource HThumbInnerBorderBrush}"/>
                                        </Border>
                                    </Border>
                                </Grid>
                            </ContentControl>
                            <ContentControl x:Name="HorizontalScaleLeftElement" Cursor="SizeWE" HorizontalContentAlignment="Stretch" Style="{StaticResource HorizontalScaleLeftStyle}" VerticalContentAlignment="Stretch" Width="8" Margin="0" Canvas.Left="142" Height="19"/>
                            <ContentControl x:Name="HorizontalScaleRightElement" Cursor="SizeWE" HorizontalContentAlignment="Stretch" Style="{StaticResource HorizontalScaleRightStyle}" VerticalContentAlignment="Stretch" Width="8" Height="19"/>
                        </Canvas>
                        <Canvas x:Name="VerticalRootCanvasElement" Background="{TemplateBinding Background}">
                            <Grid x:Name="VerticalScrollbarElement">
                                <Border Background="{StaticResource VScrollbarElementBackgroundBrush}" BorderBrush="{StaticResource VScrollbarElementBorderBrush}" BorderThickness="1" CornerRadius="2">
                                    <Border BorderBrush="{StaticResource VScrollbarElementInnerHighlightBorderBrush}" BorderThickness="1,1,0,1" CornerRadius="2"/>
                                </Border>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Border Margin="1,0"
                                            Grid.Row="1"
                                            BorderThickness="1,1,0,1"
                                            CornerRadius="2,0,0,2">
                                        <Border BorderThickness="1,1,0,1"
                                                CornerRadius="2,0,0,2">
                                            <Border x:Name="VerticalTrackElement"
                                                    Background="{StaticResource TransparentBrush}"
                                                    BorderThickness="1,1,0,1"
                                                    CornerRadius="1,0,0,1" />
                                        </Border>
                                    </Border>
                                    <Grid Cursor="Hand">
                                        <ContentControl x:Name="VerticalScrollTopElement"
                                                        Style="{StaticResource VerticalScrollTopStyle}">
                                            <Grid MaxHeight="9"
                                                  MaxWidth="22"
                                                  MinHeight="3"
                                                  MinWidth="6"
                                                  Height="5"
                                                  RenderTransformOrigin="0.5,0.5"
                                                  UseLayoutRounding="False" >
                                                <Path Margin="0,0,0,0"
                                                      HorizontalAlignment="Center"
                                                      VerticalAlignment="Center"
                                                      Stretch="Fill"
                                                      Opacity="1"
                                                      Data="M 44,22 C44,22 0,22 0,22 0,22 22.3,0 22.3,0 22.3,0 44,22 44,22 z"
                                                      Fill="{StaticResource OPDGlyphsBackgroundBrush}" Width="9" Height="6" />
                                            </Grid>
                                        </ContentControl>
                                    </Grid>
                                    <Grid Cursor="Hand" Grid.Row="2">
                                        <ContentControl x:Name="VerticalScrollBottomElement"
