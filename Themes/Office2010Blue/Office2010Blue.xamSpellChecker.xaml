  <!-- ********************************** DictionaryLoadProgressDialog Style **********************************-->
  <Style x:Key="DictionaryLoadProgressDialogStyle" TargetType="ig:DictionaryLoadProgressDialog">
    <Setter Property="Background" Value="{StaticResource ProgressDialogBackgroundBrush}" />
    <Setter Property="BorderBrush" Value="{StaticResource ProgressDialogBorderBrush}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="Padding" Value="10" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="ig:DictionaryLoadProgressDialog">
          <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Padding="{TemplateBinding Padding}">
            <StackPanel>
              <TextBlock Foreground="{StaticResource ForegroundBrush}" Text="{Binding Loading}" TextWrapping="Wrap" />
              <Border Margin="0,5,0,0">
                <ContentControl>
                  <ProgressBar Value="{TemplateBinding ProgressValue}" Height="20" Width="200" Style="{StaticResource ProgressBarStyle}" />
                </ContentControl>
              </Border>
            </StackPanel>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="ig:DictionaryLoadProgressDialog" BasedOn="{StaticResource DictionaryLoadProgressDialogStyle}" />

  <!-- *********************************  XamSpellCheckerDialogWindow Style  ********************************* -->
