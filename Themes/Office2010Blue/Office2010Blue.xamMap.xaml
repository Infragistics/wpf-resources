    <!-- *********************************  ScaleSecondary Style  ********************************* -->
    <Style x:Key="ScaleSecondaryStyle" TargetType="Path">
        <Setter Property="Fill" Value="{StaticResource ScaleSecondaryBackgroundBrush}"/>
        <Setter Property="Stroke" Value="{StaticResource ScaleSecondaryBorderBrush}"/>
        <Setter Property="StrokeThickness" Value="1.8"/>
    </Style>

    <!-- *********************************  MapScalePane Style  ********************************* -->
    <Style x:Key="MapScalePaneStyle" TargetType="local:MapScalePane">
        <Setter Property="DisplayMode" Value="Bar"/>
        <Setter Property="IsDisplaySubdivisions" Value="True"/>
        <Setter Property="CornerRadius" Value="0, 0"/>
        <Setter Property="PrimaryStyle" Value="{StaticResource ScalePrimaryStyle}"/>
        <Setter Property="SecondaryStyle" Value="{StaticResource ScaleSecondaryStyle}"/>
        <Setter Property="TextStyle" Value="{StaticResource TextStyle}"/>
        <Setter Property="TextShadowStyle" Value="{StaticResource ShadowTextStyle}"/>
        <Setter Property="ShadowOffset" Value="1.5, 1.5"/>
        <Setter Property="ShadowStyle" Value="{StaticResource ShadowStyle}"/>
        <Setter Property="Background" Value="{StaticResource MapScalePaneBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource MapScalePaneBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="3"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:MapScalePane">
                    <Grid Margin="{TemplateBinding Padding}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:1.0" Storyboard.TargetName="Background" Storyboard.TargetProperty="Opacity" To="0.4"/>
                                        <DoubleAnimation Duration="0:0:2.0" Storyboard.TargetName="Foreground" Storyboard.TargetProperty="Opacity" To="0.6"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:0.4" Storyboard.TargetName="Background" Storyboard.TargetProperty="Opacity" To="1"/>
                                        <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="Foreground" Storyboard.TargetProperty="Opacity" To="1"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="Background" 
                                CornerRadius="0"
                                Background="{TemplateBinding Background}"/>
                        <Border x:Name="Foreground" 
                                CornerRadius="0"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Padding="{TemplateBinding Padding}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <ContentPresenter Grid.Row="0" Content="{TemplateBinding Content}"/>
                                <ContentPresenter Grid.Row="1" x:Name="RootElement"/>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="local:MapScalePane" BasedOn="{StaticResource MapScalePaneStyle}"/>

    <!-- *********************************  ColorSwatchSwatch Style  ********************************* -->
    <Style x:Key="ColorSwatchSwatchStyle" TargetType="Path">
        <Setter Property="StrokeThickness" Value="1"/>
        <Setter Property="Fill" Value="{StaticResource ColorSwatchSwatchBackgroundBrush}"/>
        <Setter Property="Stroke" Value="{StaticResource ColorSwatchSwatchBorderBrush}"/>
    </Style>

    <!-- *********************************  MapColorSwatchPane Style  ********************************* -->
    <Style x:Key="MapColorSwatchPaneStyle" TargetType="local:MapColorSwatchPane">
        <Setter Property="DisplayMode" Value="Swatch"/>
        <Setter Property="LabelFormatString" Value="{}{0:F0}"/>
        <Setter Property="SwatchStyle" Value="{StaticResource ColorSwatchSwatchStyle}"/>
        <Setter Property="LeaderStyle" Value="{StaticResource ColorSwatchSwatchStyle}"/>
        <Setter Property="TextStyle" Value="{StaticResource TextStyle}"/>
        <Setter Property="TextShadowStyle" Value="{StaticResource ShadowTextStyle}"/>
        <Setter Property="ShadowOffset" Value="1.5, 1.5"/>
        <Setter Property="ShadowStyle" Value="{StaticResource ShadowStyle}"/>
        <Setter Property="Background" Value="{StaticResource MapColorSwatchPaneBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource MapColorSwatchPaneBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="3"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:MapColorSwatchPane">
                    <Grid Margin="{TemplateBinding Padding}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:1.0" Storyboard.TargetName="Background" Storyboard.TargetProperty="Opacity" To="0.5"/>
                                        <DoubleAnimation Duration="0:0:2.0" Storyboard.TargetName="Foreground" Storyboard.TargetProperty="Opacity" To="0.7"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:0.4" Storyboard.TargetName="Background" Storyboard.TargetProperty="Opacity" To="1"/>
                                        <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="Foreground" Storyboard.TargetProperty="Opacity" To="1"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="Background" 
                                CornerRadius="0" 
                                Background="{TemplateBinding Background}"/>
                        <Border x:Name="Foreground" 
                                CornerRadius="0" 
                                Background="{StaticResource TransparentBrush}" 
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Padding="{TemplateBinding Padding}">
                            <Grid >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <ContentPresenter Grid.Row="0" Content="{TemplateBinding Content}"/>
                                <ContentPresenter Grid.Row="1" x:Name="RootElement"/>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="local:MapColorSwatchPane" BasedOn="{StaticResource MapColorSwatchPaneStyle}"/>

    <!-- *********************************  ThumbnailWorld Style  ********************************* -->
    <Style x:Key="ThumbnailWorldStyle" TargetType="Path">
        <Setter Property="Fill" Value="{StaticResource ThumbnailWorldBackgroundBrush}"/>
        <Setter Property="Stroke" Value="{StaticResource ThumbnailWorldBorderBrush}"/>
        <Setter Property="StrokeThickness" Value="1"/>
    </Style>

    <!-- *********************************  ThumbnailWindow Style  ********************************* -->
    <Style x:Key="ThumbnailWindowStyle" TargetType="Path">
        <Setter Property="Fill" Value="{StaticResource ThumbnailWindowBackgroundBrush}"/>
        <Setter Property="Stroke" Value="{StaticResource ThumbnailWindowBorderBrush}"/>
        <Setter Property="StrokeThickness" Value="1"/>
    </Style>

    <!-- *********************************  MapThumbnailPane Style  ********************************* -->
    <Style x:Key="MapThumbnailPaneStyle" TargetType="local:MapThumbnailPane">
        <Setter Property="WorldStyle" Value="{StaticResource ThumbnailWorldStyle}"/>
        <Setter Property="WindowStyle" Value="{StaticResource ThumbnailWindowStyle}"/>
        <Setter Property="PreviewStyle" Value="{StaticResource PreviewStyle}"/>
        <Setter Property="Background" Value="{StaticResource MapThumbnailPaneBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource MapThumbnailPaneBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="3"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:MapThumbnailPane">
                    <Grid x:Name="RootVisual" Margin="{TemplateBinding Padding}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:1.0" Storyboard.TargetName="BackgroundVisual" Storyboard.TargetProperty="Opacity" To="0.4"/>
                                        <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="MouseOverVisual" Storyboard.TargetProperty="Opacity" To="0.0"/>
                                        <DoubleAnimation Duration="0:0:2.0" Storyboard.TargetName="RootElementVisual" Storyboard.TargetProperty="Opacity" To="0.8"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="BackgroundVisual" Storyboard.TargetProperty="Opacity" To="1"/>
                                        <DoubleAnimation Duration="0:0:1.0" Storyboard.TargetName="MouseOverVisual" Storyboard.TargetProperty="Opacity" To="1.0"/>
                                        <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="RootElementVisual" Storyboard.TargetProperty="Opacity" To="1"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="ActionStates">
                                <VisualState x:Name="Idle">
                                    <Storyboard/>
                                </VisualState>
                                <VisualState x:Name="Panning">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:0.1" Storyboard.TargetName="PressedVisual" Storyboard.TargetProperty="Opacity" To="1.0"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="FocusVisual" Storyboard.TargetProperty="Opacity" To="1.0"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Unfocused">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="FocusVisual" Storyboard.TargetProperty="Opacity" To="0.0"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid x:Name="BackgroundVisual">
                            <Border x:Name="NormalVisual" 
                                    Background="{TemplateBinding Background}"
                                    BorderBrush="{TemplateBinding BorderBrush}" 
                                    BorderThickness="{TemplateBinding BorderThickness}"/>
                            <Border x:Name="MouseOverVisual" 
                                    CornerRadius="0" Opacity="0" 
                                    Background="{StaticResource MapThumbnailPaneHoverBackgroundBrush}" 
                                    BorderThickness="{TemplateBinding BorderThickness}"/>
                            <Border x:Name="PressedVisual" 
                                    CornerRadius="0" Opacity="0" 
                                    Background="{StaticResource MapThumbnailPaneHoverBackgroundBrush}" 
                                    BorderThickness="{TemplateBinding BorderThickness}"/>
                            <Border x:Name="FocusVisual" 
                                    CornerRadius="0" Opacity="0" 
                                    BorderThickness="{TemplateBinding BorderThickness}"/>
                        </Grid>
                        <Border x:Name="RootElementVisual" 
                                Opacity="1" 
                                BorderThickness="{TemplateBinding BorderThickness}" 
                                Padding="{TemplateBinding Padding}">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <ContentPresenter Grid.Row="0" Content="{TemplateBinding Content}"/>
                                <ContentPresenter Grid.Row="1" x:Name="RootElement"/>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="local:MapThumbnailPane" BasedOn="{StaticResource MapThumbnailPaneStyle}"/>

