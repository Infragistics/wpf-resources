            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="local:MapZoomPane" BasedOn="{StaticResource MapZoomPaneStyle}" />

    <!-- *********************************  MapPanPane Style / WPF Modifications  ********************************* -->
