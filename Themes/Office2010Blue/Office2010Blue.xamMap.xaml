    <!-- *********************************  ScalePrimary Style  ********************************* -->
    <Style x:Key="ScalePrimaryStyle" TargetType="Path">
        <Setter Property="Fill" Value="{StaticResource ScalePrimaryBackgroundBrush}"/>
        <Setter Property="Stroke" Value="{StaticResource ScalePrimaryBorderBrush}"/>
