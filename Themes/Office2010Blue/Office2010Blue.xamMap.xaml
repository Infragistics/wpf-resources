                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="local:MapPanPane" BasedOn="{StaticResource MapPanPaneStyle}"/>

    <!-- *********************************  MapNavigationPane Style / WPF Modifications  ********************************* -->
