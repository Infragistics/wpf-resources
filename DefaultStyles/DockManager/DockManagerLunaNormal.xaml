                        <Border x:Name="Highlight" Visibility="Hidden" CornerRadius="2,2,0,0" BorderThickness="0,1,0,0" Background="{DynamicResource {x:Static igDock:DockManagerBrushKeys.TabbedListHighlightFillKey}}" BorderBrush="{DynamicResource {x:Static igDock:DockManagerBrushKeys.TabbedListHighlightBorderFillKey}}" Width="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Top" SnapsToDevicePixels="True" Height="3"/>
                        <ContentPresenter SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" RecognizesAccessKey="True"/>
