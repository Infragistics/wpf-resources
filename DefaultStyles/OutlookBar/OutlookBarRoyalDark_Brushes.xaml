     <Color x:Key="Color_005">#FF444444</Color><!-- Slider Background-->
     <Color x:Key="Color_006">#FF888888</Color><!-- Scrollbar Background / RadioButton & Checkbox Hover -->
     <Color x:Key="Color_007">#FF212121</Color><!-- ColorPicker/Menu Background /-->
     <Color x:Key="Color_008">#FF272727</Color><!-- Grid Lines -->
     <Color x:Key="Color_009">#FF1B1B1B</Color><!-- Calendar Background -->
