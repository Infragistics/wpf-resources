    <Color x:Key="Color_041">#BFFFFFFF</Color><!--75% White-->
    <Color x:Key="Color_042">#99FFFFFF</Color><!--60% White-->
    <Color x:Key="Color_043">#72FFFFFF</Color><!--45% White-->
