                <GradientStop Color="#FFffe896" Offset="0.49900001287460327" />
                <GradientStop Color="#FFffd975" Offset="0.5" />
                <GradientStop Color="#FFffe7a5" Offset="1" />
            </GradientStopCollection>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{x:Static igEditors:EditorsBrushKeys.DropdownBtnPressedOuterBorderFillKey}"  StartPoint="0.5,0" EndPoint="0.5,1">
        <LinearGradientBrush.GradientStops>
