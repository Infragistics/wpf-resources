    <SolidColorBrush x:Key="{x:Static igEditors:EditorsBrushKeys.DropdownBtnHottrackOuterBorderFillKey}" PresentationOptions:Freeze="true" Color="{StaticResource Color_010}" />   
    <SolidColorBrush x:Key="{x:Static igEditors:EditorsBrushKeys.DropdownBtnHottrackCenterFillKey}" PresentationOptions:Freeze="true" Color="{StaticResource Color_002}" />   
    <SolidColorBrush x:Key="{x:Static igEditors:EditorsBrushKeys.DropdownBtnPressedOuterBorderFillKey}" PresentationOptions:Freeze="true" Color="{StaticResource Color_011}" />   
    <SolidColorBrush x:Key="{x:Static igEditors:EditorsBrushKeys.DropdownBtnGlyphNormalForegroundFillKey}" PresentationOptions:Freeze="true" Color="{StaticResource Color_004}" />   
    <SolidColorBrush x:Key="{x:Static igEditors:EditorsBrushKeys.DropdownBtnGlyphNormalBackgroundFillKey}" PresentationOptions:Freeze="true" Color="{StaticResource Color_046}" />   
    <SolidColorBrush x:Key="{x:Static igEditors:EditorsBrushKeys.DropdownBtnGlyphHottrackForegroundFillKey}" PresentationOptions:Freeze="true" Color="{StaticResource Color_001}" />    
    <SolidColorBrush x:Key="{x:Static igEditors:EditorsBrushKeys.DropdownBtnGlyphHottrackBackgroundFillKey}" PresentationOptions:Freeze="true" Color="{StaticResource Color_046}" />   
-->
    
