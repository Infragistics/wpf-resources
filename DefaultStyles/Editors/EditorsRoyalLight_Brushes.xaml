    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type igEditors:XamComboEditor}, ResourceId=PopupBorderBrush}" PresentationOptions:Freeze="true" Color="{StaticResource Color_012}" />
