    <FontWeight x:Key="FontNormal">Normal</FontWeight>
    <FontWeight x:Key="FontSemiBold">SemiBold</FontWeight>
    <sys:Double x:Key="DefaultMinHeight">24</sys:Double>
    <sys:Double x:Key="DefaultSize">24</sys:Double>

    <CornerRadius x:Key="{x:Static igEditors:EditorsBrushKeys.SpinButtonCornerRadiusKey}" BottomLeft="1" BottomRight="1" TopLeft="1" TopRight="1" />
    <CornerRadius x:Key="{x:Static igEditors:EditorsBrushKeys.TextEditorCornerRadiusKey}" BottomLeft="1" BottomRight="1" TopLeft="1" TopRight="1" />
    <Thickness x:Key="{x:Static igEditors:EditorsBrushKeys.TextEditorBorderThicknessKey}" Bottom="1" Left="1" Right="1" Top="1" />
    <Thickness x:Key="{x:Static igEditors:EditorsBrushKeys.TextEditorMarginKey}" Bottom="1" Left="1" Right="1" Top="1" />
    <Thickness x:Key="{x:Static igEditors:EditorsBrushKeys.TextEditorPaddingKey}" Bottom="2" Left="6" Right="6" Top="2" />
    
