    <SolidColorBrush x:Key="DisabledForegroundBrush" PresentationOptions:Freeze="true" Color="{StaticResource Color_007}" />
    <SolidColorBrush x:Key="TransparentBrush" PresentationOptions:Freeze="true" Color="{StaticResource Color_046}" />
