    <!--  Theme Accent colors  -->  
    <Color x:Key="Color_011">#FF7446B9</Color> <!--  BG SELECTED/PRESSED/CHECKED  -->    
    <Color x:Key="Color_013">#FF7446B9</Color>  <!--  BrD FOCUS  -->

    <!--  Transparent and Whites with transparency  -->
